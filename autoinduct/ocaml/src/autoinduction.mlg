DECLARE PLUGIN "autoinduct-plugin.plugin"

{

  open Ltac_plugin
  open Stdarg
  open Autoinduct

}

(*
 * This is where the autoinduct tactic is implemented.
 *)
TACTIC EXTEND autoinduct_tactic
| [ "autoinduct" "on" constr(f) ] -> { autoinduct (Some f) }
| [ "autoinduct" ] -> { autoinduct None }
END
